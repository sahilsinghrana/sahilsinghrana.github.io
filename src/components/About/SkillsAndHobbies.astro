---
import PillList from "@components/General/Pills/PillList.astro";

const skills = [
  ["Javascript", "https://www.java.com/en/"],
  ["Typescript", "https://www.typescriptlang.org/"],
  ["Python", "https://www.python.org/"],
  ["JAVA", "https://www.java.com/en/"],
  ["React.js", "https://react.dev/"],
  ["Next.js", "https://nextjs.org/"],
  ["Express.js", "https://expressjs.com/"],
  ["MongoDB", "https://www.mongodb.com/"],
];
const hobbies = [
  ["Fading into music"],
  ["Staring Nature"],
  ["Navigating through tech"],
  ["Running with football"],
];
---

<>
  <PillList title="Tech I Understand" className="Skills" pills={skills} />
  <PillList
    title="Materials I am attached to"
    className="Hobbies"
    pills={hobbies}
  />
</>
