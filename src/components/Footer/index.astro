---
import Social from "./Social.astro";
import Nav from "./Nav.astro";
import Copyright from "./Copyright.astro";
import "./index.scss";
import fluteflower from "./fluteflower.png";
import { Image } from "astro:assets";
---

<footer>
  <div class="wave"></div>

  <Nav />
  <Social />
  <Copyright />
  <Image
    src={fluteflower}
    alt={"fluteflower"}
    class="fluteImage"
    width={250}
    loading={"lazy"}
  />
</footer>
